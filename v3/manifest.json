{
  "name": "Media Downloader Unleashed!!",
  "manifest_version": 3,
  "version": "0.5.4",
  "description": "__MSG_description__",
  "default_locale": "en",
  "permissions": [
    "storage",
    "contextMenus",
    "webRequest",
    "declarativeNetRequestWithHostAccess",
    "scripting",
    "unlimitedStorage",
    "alarms"
  ],
  "host_permissions": [
    "*://*/*.flv*",
    "*://*/*.avi*",
    "*://*/*.wmv*",
    "*://*/*.mov*",
    "*://*/*.mp4*",
    "*://*/*.pcm*",
    "*://*/*.wav*",
    "*://*/*.mp3*",
    "*://*/*.aac*",
    "*://*/*.ogg*",
    "*://*/*.wma*",
    "*://*/*.m3u8*"
  ],
  "homepage_url": "https://github.com/helloyanis/live-stream-downloader-unleashed",
  "background": {
    "scripts": [
      "network/core.js",
      "context.js",
      "plugins/blob-detector/core.js",
      "worker.js"
    ]
  },
  "icons": {
      "96": "/data/icons/download_off.svg",
      "128": "/data/icons/download_off.svg"
  },
  "action": {
    "default_title": "Download media"
  }
}