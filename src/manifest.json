{
  "name": "Media Downloader Unleashed!!",
  "manifest_version": 3,
  "version": "0.5.5",
  "description": "Download videos, music and live streams to your device!",
  "permissions": [
    "storage",
    "downloads",
    "contextMenus",
    "webRequest",
    "scripting",
    "unlimitedStorage"
  ],
  "host_permissions": [
    "*://*/*.flv*",
    "*://*/*.avi*",
    "*://*/*.wmv*",
    "*://*/*.mov*",
    "*://*/*.mp4*",
    "*://*/*.pcm*",
    "*://*/*.wav*",
    "*://*/*.mp3*",
    "*://*/*.aac*",
    "*://*/*.ogg*",
    "*://*/*.wma*",
    "*://*/*.m3u8*"
  ],
  "homepage_url": "https://github.com/helloyanis/live-stream-downloader-unleashed",
  "background": {
    "scripts": [
      "addOnOpener.js",
      "requestListener.js"
    ]
  },
  "icons": {
      "96": "icons/file_save.svg",
      "128": "icons/file_save.svg"
  },
  "action": {
    "default_title": "Download media"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "{dab9a10d-f9c5-4e8d-9351-67531e85b508}"
    }
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval' blob:; worker-src 'self' blob:"
  }
}